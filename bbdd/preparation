CREATE DATABASE IF NOT EXISTS tv-audience_landing COMMENT 'BBDD usada para cargar los datos procesados' location = '/tfm/tv-audience/preparation'

CREATE TABLE IF NOT EXISTS  `tv_audience`(
  `channel_id` int, 
  `genre_id` int, 
  `subgenre_id` int, 
  `user_id` string, 
  `program_id` string, 
  `event_id` string, 
  `duration` int)
COMMENT 'Tabla con los datos en crudo de tv_audience'
PARTITIONED BY (`week` int,`slot` int) 
ROW FORMAT DELIMITED
FIELDS TERMINATED BY ','
